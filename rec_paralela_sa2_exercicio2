var1 = float(input("Digite um valor: "))
var2 = float(input("Digite um valor: "))
var3 = float(input("Digite um valor: "))
var4 = float(input("Digite um valor: "))

media = (var1 + var2 + var3 + var4) / 4
print("Média = ",media)
if media >= 1:
    print("A média é positiva")
else:
    print("A média é negativa")
